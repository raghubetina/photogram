<%= form_with(model: @vote, local: true) do |f| %>

  <%= f.hidden_field :user_id, value: @user.id %>

  <div class="form-group">
    <%= f.label :photo_id %>
    <%= f.select :photo_id, options_from_collection_for_select(Photo.all, :id, :to_s, @vote.photo_id), {}, class: "form-control" %>
  </div>

  <%= f.submit "Create vote", class: "btn btn-block btn-outline-secondary" %>
<% end %>
